/*
    Return Statement Test #1 

    This tests if an empty return statement works. 

    Output:
        a = 16, b = 2 
*/

def func(out a:Int) => Void {
    if(a==2) {
        return 
    }
    set a *= 2
    return 
}

def a:Int = 8
def b:Int = 2
func(a)
func(b)

cout << 'a = ' << a << ', b = ' << b 

