/*
    Function Template Runtime Test #2

    This tests if a template function can be instantiated
    while passing an argument.

    Output:
        My value is 3
*/

def func<discr t>(in val:t) => Void {
    def a:t = val
    cout << 'My value is ' << a 
}

func@Int@(3)

def func<discr t>(in val:Int) => Void {
    def a:t = val 
    cout << 'MY value is ' << a 
}
