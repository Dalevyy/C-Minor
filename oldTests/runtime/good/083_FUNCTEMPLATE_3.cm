/*
    Function Template Runtime Test #3

    This tests if the correct template function will be called 
    base on when it's declared during interpretation mode.

    Output:
        Non-Specific Value is 3 
        Specific Value is 5

*/

def func<discr t>(in val:t) => Void {
    def a:t = val
    cout << 'Non-Specific Value is ' << a 
}

func@Int@(3)

def func<discr t>(in val:Int) => Void {
    def a:t = val 
    cout << 'Specific Value is ' << a 
}

func@Int@(5)
