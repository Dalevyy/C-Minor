/*
    Function Declaration Test #2

    This tests if a function can be invoked 
    with a parameter.

    Output:
        In func(), a = 8
        Outside func(), a = 3 
*/

def func(in a:Int) => Void {
    set a += 5 
    cout << 'In func(), a = ' << a 
}

def a:Int = 3
func(a)
cout << 'Outside func(), a = ' << a 
