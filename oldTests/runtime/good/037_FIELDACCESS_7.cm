/*
    Field Expression Test #7

    This tests if we use a complex field 
    expression to access Int and Array fields.

    Output:
        x = 2
        a = 1 2 3

        x = 7
        a = 4 5 6
*/

class C {
    protected x:Int
    protected a:Array[Int]

    public method create() => C { return new C(x=7, a=Array(4,5,6)) }
    public method print() => Void { 
        cout << 'x = ' << x << endl 
        cout << 'a = '
        for(def i:Int in 1..3) {
            cout << a[i] << ' '
        }
        cout << endl 
    }
}

def c:C = new C(x=2, a=Array(1,2,3))
c.print()
c.create().print()
