/*
    Binary Expression Test #2

    This tests the equality operators.

    Output:
        True == False = false
        True == True = true
        True != True = false
        False != True = true

        'hello' == 'hello' = true
        'hello' == 'world' = false
        'hello' != 'hello' = false
        'hello' != 'world' = true

        3 == 4 = false
        4 == 4 = true
        4 != 4 = false
        3 != 4 = true

        'c' == 'c' = true
        'c' == 'd' = false
        'c' != 'c' = false
        'c' != 'd' = true

        3.14 == 4.14 = false
        4.14 == 4.14 = true
        4.14 != 4.14 = false
        3.14 != 4.14 = true
*/

// Bools
cout << 'True == False = ' << (True == False)
cout << 'True == True = ' << (True == True) 
cout << 'True != True = ' << (True != True)
cout << 'False != True = ' << (False != True) << endl

// Strings 
cout << '\'hello\' == \'hello\' = ' << ('hello' == 'hello')
cout << '\'hello\' == \'world\' = ' << ('hello' == 'world')
cout << '\'hello\' != \'hello\' = ' << ('hello' != 'hello')
cout << '\'hello\' != \'world\' = ' << ('hello' != 'world') << endl

// Ints 
cout << '3 == 4 = ' << (3 == 4)
cout << '4 == 4 = ' << (4 == 4)
cout << '4 != 4 = ' << (4 != 4) 
cout << '3 != 4 = ' << (3 != 4) << endl

// Chars 
cout << '\'c\' == \'c\' = ' << ('c' == 'c') 
cout << '\'c\' == \'d\' = ' << ('c' == 'd') 
cout << '\'c\' != \'c\' = ' << ('c' != 'c') 
cout << '\'c\' != \'d\' = ' << ('c' != 'd') << endl

// Reals 
cout << '3.14 == 4.14 = ' << (3.14 == 4.14)
cout << '4.14 == 4.14 = ' << (4.14 == 4.14) 
cout << '4.14 != 4.14 = ' << (4.14 != 4.14) 
cout << '3.14 != 4.14 = ' << (3.14 != 4.14) << endl
