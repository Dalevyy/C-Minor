/*
    Unary Expression Test #1

    This tests if unary expressions work.

    Output:
        ~1 = -2
        ~-3 = 2
        ~30 = -31

        ~'a' = -98
        ~'A' = -66

        not True = false
        not False = true
*/

// Int 
cout << '~1 = ' << ~1
cout << '~-3 = ' << ~-3
cout << '~30 = ' << ~30 << endl

// Char
cout << '~\'a\' = ' << ~'a'
cout << '~\'A\' = ' << ~'A' << endl 

// Bool 
cout << 'not True = ' << not True 
cout << 'not False = ' << not False 
