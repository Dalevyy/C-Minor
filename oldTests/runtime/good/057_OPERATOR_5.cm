/*
    Operator Overloading Test #5

    This tests if a binary operator overload works.

    Output:
        3
*/

class A {
    property x:Int 

    public operator+(in obj:A) => A {
        def y:Int = x + obj.get_x() 
        return new A(x=y)
    }

    public method print() => Void { cout << x << endl }
}

def a1:A = new A(x=1)
def a2:A = new A(x=2)

def a3:A = a1 + a2
a3.print()
