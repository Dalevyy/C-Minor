/*
    Field Expression Test #8

    This tests if we can access an object 
    from an array stored by a separate object.

    Output:
        x = 2
*/

class A {
    protected x:Int 
    public method print() => Void {
        cout << 'x = ' << x << endl 
    }
}

class B {
    public arr:Array[A] 
}

def b:B = new B(arr=Array(new A(x=1),new A(x=2)))
b.arr[2].print()
