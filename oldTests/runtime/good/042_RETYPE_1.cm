/*  
    Retype Statement Test #1

    This tests if a retype statement can 
    be executed.

    Output: 
        a.x = 3
        a.y = 3.14
*/

class A {
    public x:Int
}

class B inherits A {
    public y:Real
}

def a:A = new A(x=5)
retype a = new B(x=3,y=3.14)

cout << 'a.x = ' << a.x
cout << 'a.y = ' << a.y
