/*
    New Expression Test #2

    This tests if an object can be
    instantiated with multiple fields. 

    Output:
        a.a = 1
        a.b = 3.14
        a.c = hello
        a.d = true
*/

class A {
    public a:Int
    public b:Real 
    public c:String 
    public d:Bool 
}

def a:A = new A(d=True,b=3.14,c='hello',a=1)

cout << 'a.a = ' << a.a 
cout << 'a.b = ' << a.b 
cout << 'a.c = ' << a.c
cout << 'a.d = ' << a.d 
