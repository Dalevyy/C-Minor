
def global a = 28, b = 30 Int

def addTwoNums(in e Int, in f Int) => Int {
    //def local a = 29 Int
    return a+b
}


def multiplyTwoReals(in c Real, in d Real) => Real {
    def e = 0.0 Real
    if(e < c) {
        set c += 2
    }
    else {
        set d -= 2.0
    }
    set e = c*d
    return e
}

def pure addTwoNums(in e Real, in f Real) => Real {
    return e+f
}

def main() => Void {
