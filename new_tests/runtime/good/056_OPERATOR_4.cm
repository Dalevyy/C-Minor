/*
    Operator Overloading Test #4

    This tests if a binary operator overload works
    when passing in an integer argument.

    Output:
        After overload: 0
*/

class A {
    property x:Int 

    public operator-(in val:Int) => Int {
        return x - val
    }

    public method print() => Void { cout << x << endl }
}

def a:A = new A(x=1)
def b:Int = uninit 
set b = a - 1
cout << 'After overload: ' << b << endl
