/*
    Class Template Runtime Test #3

    This tests if a class can be instantiated using 
    another class as a type argument.

    Output:
        My number is 5
*/

class A {
    protected a:Int 

    public method print() => Void {
        cout << 'My number is ' << a 
    }
}

class B<class T> {
    protected a:T 
}

def a:B<A> = new B<A>(a=new A(a=5))

a.a.print()
