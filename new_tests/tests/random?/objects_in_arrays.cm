/*
    This program checks if we are able to store
    and access objects into C Minor arrays 
*/

class A {
    protected x:Int 
    protected y:Real

    public method print() => Void {
        cout << 'x = ' << x << endl 
        cout << 'y = ' << y << endl 
    }
}

def arr:Array[A] = Array(new A(x=3,y=3.14))
arr[1].print()
