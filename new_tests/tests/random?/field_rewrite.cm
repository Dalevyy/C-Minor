
class A {
    protected a:Int 
    protected b:Real
    protected c:Bool
    protected d:Char
    protected e:String
    protected f:Array[Int]

    public method print() => Void {
        cout << 'a = ' << a << endl
             << 'b = ' << b << endl
             << 'c = ' << c << endl
             << 'd = ' << d << endl
             << 'e = ' << e << endl
             << 'f[1] = ' << f[1] << endl
    }
}

def a:A = new A(a=1,b=3.14,c=True,d='z',e='hello',f=Array(7,8))
a.print()
