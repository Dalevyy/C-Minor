
def l1:List[Int] = List(1,2,3,4,5)
def l2:List[Int] = List()

append(l2,6)            // l2 = {6}
append(l1,l2)           // l1 = {1,2,3,4,5,6}
insert(l1,3,100)        // l1 = {1,2,100,3,4,5,6}
remove(l1,100)          // l1 = {1,2,3,4,5,6}
remove(l1,List(3,4,5))  // l1 = {1,2,6}
length(l1)              // 3 
